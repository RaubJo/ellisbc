---
const { title } = Astro.props;
---
<section class="text-gray-600 bg-gray-920 body-font">
  <div class="container px-2 py-8 mx-auto flex flex-col">
    <h1 class="mx-auto font-bold text-black text-4xl">{ title }</h1>
    <div class="lg:w-4/6 mx-auto mt-6">

      <slot></slot>
    
    </div>
  </div>
</section>
