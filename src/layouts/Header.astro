---
import NavLink from "../components/NavLink.astro";
import NavBar from "../components/NavBar.astro";

import FacebookProfile from "../links/FacebookProfile.astro"
import PushPay from "../links/PushPay.astro";

const media = FacebookProfile + "/videos/"
---

<section class="w-full mb-4 antialiased bg-white">
    <div class="mx-auto max-w-7xl">
        <NavBar>
            <NavLink href="/">Home</NavLink>
            <NavLink href="/about/us">About Us</NavLink>
            <NavLink href={media}>Media</NavLink>
            <NavLink href="/contact/us">Contact Us</NavLink>
            <NavLink href={PushPay}>Give Online</NavLink>
        </NavBar>
    </div>
</section>