{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@icons/*": ["src/icons/*"],
      "@cms/*": ["src/cms/*"],
      "@images/*": ["src/images/*"],
      "react": [ "./node_modules/@types/react" ]
    }
  },
}